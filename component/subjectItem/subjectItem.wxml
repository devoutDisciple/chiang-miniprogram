<view class="data" bindtap = "onClick">
    <view class="data_title">{{subject.title}}</view>
    <view class="data_time">课程周期：{{subject.start_time}} - {{subject.end_time}}</view>
    <view class="data_tag">
        <view class="class_tag tag">22考研</view>
        <view class="desc_tag tag">直播</view>
        <view class="desc_tag tag">10小时</view>
        <view class="desc_tag tag">备考秘诀</view>
        <view class="desc_tag tag">阅卷人亲授</view>
    </view>
    <view class="data_person">
        <image class="data_person_img" src="/asserts/temp/photo.png" />
        <view class="data_person_name">罗老师</view>
        <view class="data_person_desc">限时优惠3000元</view>
    </view>
</view>